<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace = "com.spring.elderlycare.dao.DatasDAOImpl">

<select id = "selectHumids" parameterType = "integer" resultType = "list">
	SELECT measuredtime, humid FROM realtimedata 
	WHERE elderly = #{value} AND 
	<![CDATA[DATE(measuredtime) = DATE(NOW())]]> AND humid is not NULL;
</select>
<select id = "selectTemps" parameterType = "integer" resultType = "list">
	SELECT measuredtime, temp FROM realtimedata 
	WHERE elderly = #{value} AND 
	<![CDATA[DATE(measuredtime) = DATE(NOW())]]> AND temp is not NULL;
</select>
</mapper>